

<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
</head>

<body>
    <div class="page">
        <div id="header">
            <div id="title">
                <h1>Trail Locker</h1>
            </div>
            <div id="logindisplay">
                You are logged in as @{Html.RenderAction("display_email", "Super");}
            </div>
            <div id="menu">
                <ul id="navBar">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("Locker", "Index", "Locker")</li>
                    <li>@Html.ActionLink("Trips", "Index", "Trip")</li>
                    <li>@Html.ActionLink("User", "Index", "User")</li>
                    <li>@Html.ActionLink("My Account", "Account", "User")</li>
                    <li>@Html.ActionLink("Logout", "Index", "Logout")</li>
                </ul>
            </div>
        </div>

        <div id="main">
            @RenderBody()
        </div>
        <div id="footer">

        </div>
    </div>
</body>
</html>
